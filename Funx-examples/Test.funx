module Test

main = even 13

factorial : Int -> Int
factorial n = if n < 1 then 1 else n * factorial (n - 1) fi

boolCompose : (Bool -> a) -> (Bool -> Bool) -> Bool -> a
boolCompose = id compose

even x = let
even a = a == 0 || odd (a - 1)
odd a = a != 0 && even (a - 1)
compose f g x = f (g x)
in (and True) . (or False) $ even x
